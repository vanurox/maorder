<div class="row">
            <div class="col-lg-8 col-lg-offset-2">
  <div class="col-lg-8 col-lg-offset-4 col-md-8 col-md-offset-4 col-sm-8 col-sm-offset-4 col-xs-9 col-xs-offset-3">
    <%if session[:is_admin] %>
            <div class="col-lg-4 col-md-2 col-sm-2">
                    <h4>Select Seller Email</h4>
            </div>
            <div class="col-lg-6 col-md-5 col-sm-6">
                        <%= f.select("user_id", @users.collect {|u| [u.email, u.id ] }, { include_blank: "Select Email" },:class=>'form-control',:required=>true)%>
            </div>
    <% else%>
    <%= f.hidden_field("user_id",:value=>session[:user_id]) %>
   
    <% end%>
</div>
  <table class="table">
    <thead>
            <tr>
                <td>Select Product</td>
                <td><%= select("photo", "product_id", @product.collect {|p| [p.product_name, p.id ] }, { include_blank: "Select Product"},:class=>'form-control',:required=>true)%></td>


            </tr>
    </thead>
            <tr>
                <td>Choose Images[Select Maximum of 5 photos to upload]:</td>
                <td><%= f.file_field("photo1",:required=>true,class: 'btn btn-default') %></td>
        <tr><td></td><td><%= f.file_field("photo2",:required=>true,class: ' btn btn-default') %></td></tr>
        <tr><td></td><td><%= f.file_field("photo3",:required=>true,class: ' btn btn-default') %></td></tr>
        <tr><td></td><td><%= f.file_field("photo4",:required=>true,class: ' btn btn-default') %></td></tr>
        <tr><td></td><td><%= f.file_field("photo5",:required=>true,class: ' btn btn-default') %></td></tr>

            
            </tr>
</table>
</div>
</div>